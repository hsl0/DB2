{
    "compilerOptions": {
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "newLine": "lf",
        "target": "ES5",
        "forceConsistentCasingInFileNames": true,
        "outDir": ".",
        "strictBindCallApply": true,
        "strictFunctionTypes": true,
        "strictNullChecks": true,
        "alwaysStrict": true,
        "sourceMap": true,
        "module": "CommonJS",
        "declaration": true,
        "lib": [
            "DOM",
            "ES2017"
        ],
        "composite": true
    },
    "files": [
        "hybridStorage.ts"
    ]
}